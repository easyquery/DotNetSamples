{
  "name": "EqStencilDemo",
  "version": "1.0.0",
  "description": "Stencil RealWorld Example App (Conduit)",
  "scripts": {
    "build": "rimraf ./www && stencil build --prerender",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "generate": "stencil generate"
  },
  "dependencies": {
    "@stencil/core": "^2.2.0",
    "@stencil/router": "^1.0.1",
    "@easydata/core": "1.2.1",
    "@easydata/ui": "1.2.1",
    "@easyquery/core": "7.0.1",
    "@easyquery/ui": "7.0.1",
    "@easyquery/enterprise": "7.0.1"
  },
  "license": "MIT",
  "devDependencies": {
    "@stencil/sass": "^1.4.0",
    "@types/jest": "26.0.15",
    "@types/puppeteer": "^5.4.0",
    "jest": "^26.6.2",
    "jest-cli": "^26.6.2",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "puppeteer": "^5.4.1",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-stencil": "^1.0.1",
    "workbox-build": "^5.1.4"
  }
}
